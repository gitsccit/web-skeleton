{
    "name": "gitsccit/web-skeleton",
    "description": "Skeleton plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Xun Chen",
            "role": "Maintainer"
        }
    ],
    "require": {
        "cakephp/cakephp": ">=3.8"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": ">=3.2",
        "phpunit/phpunit": "6.*",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Skeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skeleton\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
